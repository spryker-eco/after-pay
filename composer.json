{
  "name": "spryker-eco/after-pay",
  "type": "library",
  "description": "AfterPay module",
  "license": "MIT",
  "require": {
    "php": ">=7.3",
    "psr/http-message": "*",
    "spryker/checkout-extension": "*",
    "spryker/customer": "*",
    "spryker/guzzle": "*",
    "spryker/kernel": "^3.30.0",
    "spryker/log": "*",
    "spryker/money": "^2.0.0",
    "spryker/payment": "^4.0.0 || ^5.0.0",
    "spryker/refund": "*",
    "spryker/sales": "^6.0.0 || ^7.0.0 || ^8.0.0 || ^10.0.0 || ^11.0.0",
    "spryker/step-engine": "^2.0.0 || ^3.0.0",
    "spryker/store": "*",
    "spryker/symfony": "^3.0.0",
    "spryker/util-encoding": "^2.0.0",
    "spryker/util-text": "^1.0.0",
    "spryker/zed-request": "^3.0.0"
  },
  "require-dev": {
    "spryker/application": "*",
    "spryker/checkout": "*",
    "spryker/code-sniffer": "*",
    "spryker/oms": "*",
    "spryker/product": "*",
    "spryker/propel": "*",
    "spryker/testify": "*"
  },
  "suggest": {
    "spryker/checkout": "If you want to use Checkout plugins you need to install spryker/checkout.",
    "spryker/installer": "If you want to use Installer plugins you need to install spryker/installer.",
    "spryker/oms": "If you want to use Oms plugins you need to install spryker/oms.",
    "spryker/silex": "If you want to use Silex plugins you need to install spryker/silex."
  },
  "autoload": {
    "psr-4": {
      "SprykerEco\\": "src/SprykerEco/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "SprykerEcoTest\\": "tests/SprykerEcoTest/"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "scripts": {
    "cs-check": "phpcs -p -s --standard=vendor/spryker/code-sniffer/Spryker/ruleset.xml src/ tests/",
    "cs-fix": "phpcbf -p --standard=vendor/spryker/code-sniffer/Spryker/ruleset.xml src/ tests/"
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0.x-dev"
    }
  },
  "config": {
    "preferred-install": "dist",
    "sort-packages": true,
    "process-timeout": 600
  }
}
